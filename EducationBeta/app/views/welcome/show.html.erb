<div id="main" class="container_12"><!-- Body Wrapper Begin -->
  <div id="header"><!-- Header Begin -->
    <div class="grid_3"><a href="#" id="logo" class="float_left">EducationBeta</a></div>
    <div class="grid_4 push_5">
      <%= render "shared/header_search"%>
    </div>
  </div><!-- Header End -->
  <div class="clear"></div>

<div id="userbar"><!-- Userbar Begin -->
</div>
<div>
<div id="body">
	<div class="block"><!-- Block Begin -->
	  <div class="titlebar">
	    <h3></h3>
	  </div>
	  <div class="block_cont">
	    <div id="chart1" class="landing_image" style="height:200px;"></div>
	  </div>
	</div>

	
	<div class="block small"><!-- Block Begin -->
	  <div class="titlebar">
	    <h3>Login</h3>
	  <div class="block_cont" style="background: url('title_txt.png') 0 0 no-repeat;">
	    <div id="chart2" style="height:200px; width:100%;">
	    	<%= form_tag sessions_path do %> 
			<div id="loginbox">
				<a href="#" id="logo">EducationBeta</a>
				<div id="loginform">
					<form name="login" action="cp_main.html" method="get">
						<div id="username_field"><%= text_field_tag :email, params[:email], :class => 'required' %></div>
						<div id="password_field"><%= password_field_tag :password, nil, :class => 'required' %></div>
						<div id="buttonline">
							<%= submit_tag "Log in", :class => 'float_left width_4', :id => 'loginbutton' %>
							<a href="#" class="passforgot float_right">Forgot your password?</a>
						</div>
					</form>	
				</div>
			</div>
			<% end %>
	    </div>
	  </div>	
	</div>
</div>	

</div>
