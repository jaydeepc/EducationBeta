 <h2> Welcome to Education Beta </h2>
<%= stylesheet_link_tag "welcome" %>
 <div id="user_nav">
     <% if current_user %>
       Logged in as <%= current_user.email %>.
       <%= link_to "Log out", log_out_path %>
     <% else %>
        <%= link_to "Sign up", sign_up_path, :id => 'user_nav' %>  
        <%= link_to "log in", log_in_path, :id => 'user_nav' %>
     <% end %>
 </div>