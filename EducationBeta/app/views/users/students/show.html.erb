<div id="main" class="container_12"><!-- Body Wrapper Begin -->
  <div id="header"><!-- Header Begin -->
    <div class="grid_3"><a href="#" id="logo" class="float_left">EducationBeta</a></div>
    <div class="grid_4 push_5">
      <%= render "shared/header_search"%>
    </div>
  </div><!-- Header End -->
  <div class="clear"></div>
  <div id="userbar"><!-- Userbar Begin -->
    <%= render "users/students/userbar/profile"%>
    <%= render "users/students/userbar/navigation"%>
  </div><!-- Userbar End -->
  <div class="clear"></div>
  <div id="body">
    <%= render :partial => "users/students/body/big_block", :locals => {:header => "Course Completion Status", :text => "Graph will come here"}%>
    <%= render :partial => "users/students/body/small_block", :locals => {:header => "My Notification", :text => "Notifications will come here"}%>
    <%= render :partial => "users/students/body/small_block", :locals => {:header => "Test Results", :text => "Test Results will come here"}%>
    <%= render :partial => "users/students/body/small_block", :locals => {:header => "My Ranking report", :text => "Report will come here"}%>
  </div><!-- Body Wrapper End -->
